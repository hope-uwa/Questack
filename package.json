{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "description": "StackOverflow-lite is a platform where people can ask questions and provide answers.",
  "main": "app.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --require babel-register server/test/*.js  --exit",
    "start": "npm run babel-node -- ./server/app.js",
    "lint-all": "eslint ./server/app.js ./server/test/*",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov mocha --reporter spec --require babel-register server/test/* --exit",
    "coveralls": "npm run coverage -- --report lcovonly && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uwaelpis/StackOverflow-lite.git"
  },
  "author": "Hope Uwa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uwaelpis/StackOverflow-lite/issues"
  },
  "homepage": "https://github.com/uwaelpis/StackOverflow-lite#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.0",
    "istanbul": "^0.4.5",
    "lcov-parse": "^1.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2"
  }
}
