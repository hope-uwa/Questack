{
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "prettier",
        "prettier/react"
    ],
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "semi": 0,
      "max-len": 0,
      "no-multiple-empty-lines": 0,
      "object-shorthand": 0,
      "padded-blocks": 0
    },
    "prettier/prettier":[
        "error",
        {
            "trailingComa":"es5",
            "singleQuote":true,
            "printWidth": 120
        }
    ],
    "plugins":[
        "prettier"
    ]
  }